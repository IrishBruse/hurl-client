{
    "name": "hurl-parser",
    "version": "1.0.0",
    "scripts": {
        "build": "node esbuild.mjs",
        "watch": "node esbuild.mjs --watch"
    },
    "author": "Ethan Conneely",
    "license": "MIT",
    "description": "Hurl parser",
    "dependencies": {},
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
