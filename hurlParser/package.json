            "require": "./dist/*.js",
            "import": "./dist/*.js"
        }
    },
    "types": "./dist/index.d.ts",
    "devDependencies": {
        "esbuild-plugin-d.ts": "^1.3.1",
        "@types/node": "^22.10.6",
        "esbuild": "0.24.2",
        "typescript": "^5.7.3"
    }
}
